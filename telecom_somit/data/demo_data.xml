<odoo>
    <data noupdate="1">
        <!-- Product category Demo Data-->
        <record id="categ_1" model="product.category">
            <field name="name">landline</field>
            <field name="code">LL</field>
        </record>

        <record id="categ_2" model="product.category">
            <field name="name">Mobile</field>
            <field name="code">MO</field>
        </record>

        <!-- Product data -->
        <record id="product_temp_1" model="product.template">
            <field name="name">ADSL</field>
            <field name="default_code">LL001</field>
            <field name="categ_id" ref="categ_1"></field>
        </record>

        <record id="product_temp_2" model="product.template">
            <field name="name">Fiber</field>
            <field name="default_code">LL002</field>
            <field name="categ_id" ref="categ_1"></field>
        </record>

        <record id="product_temp_3" model="product.template">
            <field name="name">Mobile voice</field>
            <field name="default_code">MO001</field>
            <field name="categ_id" ref="categ_2"></field>
        </record>

        <record id="product_temp_4" model="product.template">
            <field name="name">Mobile data</field>
            <field name="default_code">MO002</field>
            <field name="categ_id" ref="categ_2"></field>
        </record>

    </data>
</odoo>
